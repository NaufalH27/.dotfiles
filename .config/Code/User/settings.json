{
    "security.workspace.trust.untrustedFiles": "open",
    "files.autoSave": "afterDelay",
    "window.menuBarVisibility": "toggle",
    "editor.fontSize": 14,
    "editor.fontWeight": "500",
    "editor.lineHeight": 22,
    "editor.letterSpacing": 0.2,
    "workbench.colorTheme": "Catppuccin Mocha",
    "editor.renderWhitespace": "all",
    "editor.smoothScrolling": true,
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "smooth",
    "editor.fontFamily": "'Maple Mono SC NF' , 'Symbols Nerd Font', 'monospace', 'Noto Color Emoji'",
    "terminal.integrated.fontFamily": "'Maple Mono SC NF', 'Symbols Nerd Font', 'monospace', 'Noto Color Emoji'",
    "workbench.tree.indent" :16,
    "editor.minimap.enabled": false,
    "customizeUI.listRowHeight" : 24 ,
    "terminal.integrated.scrollback": 1000000,
    "editor.fontLigatures": true,
    "workbench.activityBar.location": "hidden",
    "workbench.iconTheme": "material-icon-theme",
    "vscode_custom_css.policy": true,
    "editor.wordWrap": "on",
    "explorer.confirmDragAndDrop": false,
    "workbench.editor.editorActionsLocation": "titleBar",
   
    "catppuccin.accentColor": "yellow",
    "editor.semanticHighlighting.enabled": true,
    "terminal.integrated.minimumContrastRatio": 1,
    "gopls": {
      "ui.semanticTokens": true,
    },
    "catppuccin.customUIColors": {
      "mocha": {
        "statusBar.foreground": "accent",
      },
    },
    "explorer.confirmPasteNative": false,
}

