        "tab.activeBackground": "#14161f",
        "tab.inactiveBackground": "#1e1e2e",
        "tab.border": "#1e1e2e",
        "tab.activeForeground": "#ffffff",
        "tab.inactiveForeground": "#919090",
        "editorGroupHeader.tabsBackground": "#1e1e2e",
        "editorGutter.background" : "#1e1e2e",
        "editor.lineHighlightBackground": "#2b2f41", 
        "editor.wordHighlightBackground": "#454a64", 
        "editor.wordHighlightStrongBackground": "#50587a"
    
    },
    
